@using ShoesShop.Models;
@model List<Category>

<span class="bottom"></span>
<h3>Categories</h3>
<div class="content">
    <ul class="sidebar_list">
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(@item.Name, "ListProductByCategoryId", "Home", new { id = @item.Id}, null)</li>
        }
    </ul>
</div>
